{
  "private": true,
  "description": "React VR Preview Bundle",
  "dependencies": {
    "aframe": "^0.4.0",
    "aframe-react": "^3.4.0",
    "diff": "*",
    "md5-file": "*",
    "ovr-audio": "~0.1.0",
    "ovrui": "~0.1.0",
    "react": "15.3.2",
    "react-dom": "^15.4.1",
    "react-native": "0.37.0",
    "react-vr": "~0.1.0",
    "react-vr-web": "~0.1.0",
    "request": "*",
